<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wm.mutildatasource.mapper.aliyun.ProductMapper">

    <insert id="insert" parameterType="com.wm.mutildatasource.model.Product">
        insert into product(product_name, stock, version)
        values(#{productName}, #{stock}, #{version})
    </insert>

</mapper>