<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wm.mutildatasource.mapper.local.UserMapper">

    <insert id="insert" parameterType="com.wm.mutildatasource.model.User">
        insert into user(name, age, sex)
        values(#{name}, #{age}, #{sex})
    </insert>

</mapper>